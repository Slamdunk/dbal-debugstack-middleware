{
    "name": "slam/dbal-debugstack-middleware",
    "description": "Doctrine\\DBAL middleware for precise query debugging (DebugStack replacement)",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.4.0 || ~8.5.0",
        "doctrine/dbal": "^4.4.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.33",
        "phpstan/phpstan-phpunit": "^2.0.11",
        "phpstan/phpstan-strict-rules": "^2.0.7",
        "phpunit/phpunit": "^12.5.5",
        "slam/php-cs-fixer-extensions": "^3.14.0"
    },
    "autoload": {
        "psr-4": {
            "Slam\\DbalDebugstackMiddleware\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamTest\\DbalDebugstackMiddleware\\": "tests/"
        }
    }
}
