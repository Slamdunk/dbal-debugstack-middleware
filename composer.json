{
    "name": "slam/dbal-debugstack-middleware",
    "description": "Doctrine\\DBAL middleware for precise query debugging (DebugStack replacement)",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.2.0 || ~8.3.0",
        "doctrine/dbal": "^4.0.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10.58",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpstan/phpstan-strict-rules": "^1.5.2",
        "phpunit/phpunit": "^10.5.10",
        "slam/php-cs-fixer-extensions": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "Slam\\DbalDebugstackMiddleware\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamTest\\DbalDebugstackMiddleware\\": "tests/"
        }
    }
}
